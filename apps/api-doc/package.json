{
  "name": "api-doc",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next .source",
    "pagefind": "npx -y pagefind --site out",
    "serve-static": "npm run clean && npm run build && npm run pagefind && npx serve out -l 1234",
    "dev-static": "nodemon --config nodemon.dev-static.json",
    "dev": "next dev --port 1234",
    "start": "next start --port ${PORT:-1234}",
    "postinstall": "fumadocs-mdx"
  },
  "dependencies": {
    "@govtechmy/myds-react": "^0.0.4",
    "@govtechmy/myds-style": "^0.0.3",
    "@uidotdev/usehooks": "^2.4.1",
    "fumadocs-core": "14.7.7",
    "fumadocs-mdx": "11.3.1",
    "fumadocs-ui": "14.7.7",
    "next": "15.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@fumadocs/cli": "^0.0.7",
    "@repo/icons": "*",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.1.5",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
